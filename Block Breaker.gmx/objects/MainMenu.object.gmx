          <argument>
            <kind>1</kind>
            <string>/// Check for mouse hover or keyboard
if(!global.no_cursor) {
    if(mouse_x &gt; 365 &amp;&amp; mouse_x &lt; 661 &amp;&amp; mouse_y &gt; 482 &amp;&amp; mouse_y &lt; 519) {
        hover = 0;
    } else if(mouse_x &gt; 404 &amp;&amp; mouse_x &lt; 623 &amp;&amp; mouse_y &gt; 520 &amp;&amp; mouse_y &lt; 569) {
        hover = 1;
    } else if(mouse_x &gt; 364 &amp;&amp; mouse_x &lt; 661 &amp;&amp; mouse_y &gt; 570 &amp;&amp; mouse_y &lt; 619) {
        hover = 2;
    }  else if(mouse_x &gt; 457 &amp;&amp; mouse_x &lt; 569 &amp;&amp; mouse_y &gt; 620 &amp;&amp; mouse_y &lt; 669) {
        hover = 3;
    }  else if(mouse_x &gt; 392 &amp;&amp; mouse_x &lt; 633 &amp;&amp; mouse_y &gt; 670 &amp;&amp; mouse_y &lt; 707) {
        hover = 4;
    } else hover = -1;
}

if(global.key_up) {
    hover -= 1;
    if(hover &lt; 0) hover = 4;
    global.no_cursor = true;
}
if(global.key_down) {
    hover += 1;
    if(hover &gt; 4) hover = 0;
    global.no_cursor = true;
}

if(global.key_fire || global.key_enter) {
    switch(hover) {
        case 0: 
            global.no_cursor = true;
            global.in_game = true;
            room_goto_next();
            break;
        case 1: 
            break;
        case 2:
            break;
        case 3:
            break;
        case 4: game_end();
    }
}
</string>
