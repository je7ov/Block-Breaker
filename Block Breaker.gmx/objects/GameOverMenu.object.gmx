          <argument>
            <kind>1</kind>
            <string>/// Check for mouse hover or keyboard
if(!global.no_cursor) {
    if(mouse_x &gt; 334 &amp;&amp; mouse_x &lt; 694 &amp;&amp; mouse_y &gt; 555 &amp;&amp; mouse_y &lt;= 592) {
        hover = 0;
    } else if(mouse_x &gt; 241 &amp;&amp; mouse_x &lt; 786 &amp;&amp; mouse_y &gt; 592 &amp;&amp; mouse_y &lt; 636) {
        hover = 1;
    } else hover = -1;
}

if(global.key_up) {
    hover -= 1;
    if(hover &lt; 0) hover = 1;
    global.no_cursor = true;
}
if(global.key_down) {
    hover += 1;
    if(hover &gt; 1) hover = 0;
    global.no_cursor = true;
}

if(global.key_fire_released || global.key_enter) {
    switch(hover) {
        case 0:
            script_execute(start_game);
            break;
        case 1:
            global.key_fire_released = false;
            global.key_enter = false;
            room_goto(rm_main_menu);
            break;
    }
}
</string>
